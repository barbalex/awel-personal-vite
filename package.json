{
  "name": "personal",
  "productName": "Personal",
  "version": "2.0.3",
  "main": "dist-electron/main/index.js",
  "description": "Damit verwaltet das AWEL Kt. ZH Personal-Daten",
  "author": {
    "name": "Alexander Gabriel",
    "email": "alex.barbalex@gmail.com"
  },
  "license": "MIT",
  "private": true,
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build_sik": "tsc && vite build && electron-builder",
    "preinstall": "cross-env-shell npm install better-sqlite3 --no-save --build-from-source --sqlite3=\"$INIT_CWD/sqlite-amalgamation\" --runtime=electron --target=25.1.1 --dist-url=https://electronjs.org/headers --force",
    "preview": "vite preview",
    "e2e": "npx playwright test"
  },
  "dependencies": {
    "@tanstack/react-query": "4.29.13",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vitejs/plugin-react": "4.0.0",
    "babel-loader": "9.1.2",
    "better-sqlite3": "file:./node_modules/better-sqlite3",
    "bootstrap": "4.6.2",
    "classnames": "2.3.2",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
