    "path": "0.12.7",
    "popper.js": "1.16.1",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-datepicker": "4.13.0",
    "react-dom": "18.2.0",
    "react-dropzone": "12.1.0",
